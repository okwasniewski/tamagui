{
  "name": "@tamagui/demos",
  "version": "1.76.0",
  "sideEffects": [
    "*.css"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/biome check src",
    "lint:fix": "../../node_modules/.bin/biome check --apply-unsafe src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./photo/*": "./public/*.jpg",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./demo/*": {
      "types": "./types/*.d.ts",
      "node": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "dependencies": {
    "@tamagui/avatar": "1.76.0",
    "@tamagui/button": "1.76.0",
    "@tamagui/core": "1.76.0",
    "@tamagui/list-item": "1.76.0",
    "@tamagui/logo": "1.76.0",
    "@tamagui/menu": "1.76.0",
    "@tamagui/popover": "1.76.0",
    "@tamagui/progress": "1.76.0",
    "@tamagui/radio-group": "1.76.0",
    "@tamagui/select": "1.76.0",
    "@tamagui/sheet": "1.76.0",
    "@tamagui/site-config": "1.76.0",
    "@tamagui/skeleton": "1.76.0",
    "@tamagui/slider": "1.76.0",
    "@tamagui/stacks": "1.76.0",
    "@tamagui/toast": "1.76.0",
    "tamagui": "1.76.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.76.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
